package pi;

public class Aguja {
	int unidad;
	int theta;
	
	Aguja() {
		
	}
	
	

	/**
	 * @return the unidad
	 */
	public int getUnidad() {
		return unidad;
	}

	/**
	 * @param unidad the unidad to set
	 */
	public void setUnidad(int unidad) {
		this.unidad = unidad;
	}

	/**
	 * @return the theta
	 */
	public int getTheta() {
		return theta;
	}

	/**
	 * @param theta the theta to set
	 */
	public void setTheta(int theta) {
		this.theta = theta;
	}
	
	
}
