package parabolico;

import java.awt.GridLayout;

import javax.swing.JFrame;

public class VistaInfo extends JFrame{
	public VistaInfo(){
		setLayout(new GridLayout());
		setLocation(300, 100); // Center the frame
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setSize(1000, 800);
		setVisible(true);
		setVisible(true);
		
	}
}
